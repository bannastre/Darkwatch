{
	"name": "darkwatch",
	"type": "dark",
	"colors": {
		"editor.background": "#282828ff",
		"editor.foreground": "#d4d4d4",
		"activityBarBadge.background": "#3d425c",
		"sideBarTitle.foreground": "#bbbbbb"
	},
	"tokenColors": [
		{
			"settings": {
				"foreground": "#eeffffff",
				"background": "#263238ff"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#546E7A"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
				// "variable.parameter"
			],
			"settings": {
				"foreground": "#83A598"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier",
				"keyword.control",
				"keyword.other",
				"keyword.other.unit",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#FE8019"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"meta.group.braces meta.group.braces.curly.js",
				"meta.group.braces.round.js",
				"meta.group.braces.round.function.arguments.js",
				"meta.group.braces.curly.js",
				"meta.group.braces.square.js",
				"support.function.node.js",
				"variable.function.js",
				"variable.other.readwrite.js"
			],
			"settings": {
				"foreground": "#fdf4c1ff"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#BDA98A"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"constant.character",
				"constant.escape",
				"entity.name"
			],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"fontStyle": "normal",
				"foreground": "#83A598"
			}
		},
		{
			"name": "Support",
			"scope": [
				"support.type",
				"support.class",
				"support.constant",
				"support.orther.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#FE8019"
			}
		},
		{
			"name": "Ruby Instance Variables",
			"scope": [
				"variable.other.readwrite.instance.ruby",
				"punctuation.definition.variable.ruby"
			],
			"settings": {
				"foreground": "#d68971"
			}
		}
	]
}